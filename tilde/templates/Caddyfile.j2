    tls {$CLOUDFLARE_EMAIL} {
        dns cloudflare {$CLOUDFLARE_API_TOKEN}
    }
    reverse_proxy {{ INTERNAL_DOMAIN }}:8096
}

portainer.{{ DOMAIN }}:443 {
    tls {$CLOUDFLARE_EMAIL} {
        dns cloudflare {$CLOUDFLARE_API_TOKEN}
    }
    reverse_proxy {{ INTERNAL_DOMAIN }}:9000
}

http://nextcloud.omashu.xyz {
    reverse_proxy {{ INTERNAL_DOMAIN }}:8080
}
